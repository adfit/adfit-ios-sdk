       tableView.register(BizBoardCell.self, forCellReuseIdentifier: adCellIdentifier)","    }","}"],"type":"codeListing"}],"title":"Swift"},{"title":"Objective-C","content":[{"code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK.h>","","@implementation MyViewController","","- (void)viewDidLoad {","    [super viewDidLoad];","    ","    \/\/ ... 구현 생략 ...","    [self.tableView registerClass:[BizBoardCell self] forCellReuseIdentifier: adCellIdentifier];","}","@end"],"syntax":"objective-c","type":"codeListing"}]}],"type":"tabNavigator"},{"text":"2. AdFitNativeAdLoaderDelegate 구현","type":"heading","anchor":"2-AdFitNativeAdLoaderDelegate-구현","level":3},{"type":"paragraph","inlineContent":[{"text":"광고 응답으로 받은 AdFitNativeAd를 property로 할당받고, 광고 응답이 왔을 경우, tableView를 reload 합니다. (loader의 호출 로직은 동일합니다.)","type":"text"}]},{"type":"tabNavigator","tabs":[{"title":"Swift","content":[{"type":"codeListing","syntax":"swift","code":["import UIKit","import AdFitSDK","","final class MyViewController: UIViewController, AdFitNativeAdLoaderDelegate, \/*... 생략 ...*\/ {","    \/\/ ... property 생략 ...","    private var nativeAd: AdFitNativeAd?","    private var nativeAdLoader: AdFitNativeAdLoader?","    ","    \/\/ ... 구현 생략 ...","    ","    \/\/ MARK: - AdFitNativeAdLoaderDelegate","    func nativeAdLoaderDidReceiveAd(_ nativeAd: AdFitNativeAd) {","        self.nativeAd = nativeAd","        tableView.reloadData()","        print(\"광고 응답을 받았습니다.\")","    }","    ","    func nativeAdLoaderDidFailToReceiveAd(_ nativeAdLoader: AdFitNativeAdLoader, error: Error) {","        tableView.reloadData()","        print(\"광고 응답 에러: \\(error) (\\(error.localizedDescription))\")","    }","}"]}]},{"content":[{"code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK.h>","","@implementation MyViewController","","#pragma mark - AdFitNativeAdLoaderDelegate","- (void)nativeAdLoaderDidReceiveAd:(AdFitNativeAd *)nativeAd {","    self.nativeAd = nativeAd;","    [self.tableView reloadData];","    NSLog(@\"광고 응답을 받았습니다.\");","}","","- (void)nativeAdLoaderDidFailToReceiveAd:(AdFitNativeAdLoader *)nativeAdLoader error:(NSError *)error {","    [self.tableView reloadData];","    NSLog(@\"광고 응답 에러: %@\", [error localizedDescription]);","}","","@end"],"type":"codeListing","syntax":"objective-c"}],"title":"Objective-C"}]},{"level":3,"type":"heading","anchor":"3-UITableViewDataSource-구현","text":"3. UITableViewDataSource 구현"},{"inlineContent":[{"type":"text","text":"cellForRowAt function에서 "},{"type":"strong","inlineContent":[{"text":"adHeight(width: CGFloat)","type":"text"}]},{"type":"text","text":" function의 결과를 adHeight property에 저장합니다."}],"type":"paragraph"},{"tabs":[{"content":[{"type":"codeListing","code":["import UIKit","import AdFitSDK","","final class MyViewController: UIViewController, \/*... 생략 ...*\/ {","    \/\/ ... property 생략 ...","    private var adHeight: CGFloat = 0","    ","    \/\/ ... 구현 생략 ...","    ","    \/\/ MARK: UITableViewDataSource","    ","    func tableView(_ tableView: UITableView, cellForRowAt indexPath: IndexPath) -> UITableViewCell {","        if let cell = tableView.dequeueReusableCell(withIdentifier: adCellName, for: indexPath) as? BizBoardCell {","            adHeight = cell.adHeight(width: view.bounds.width)","            \/\/ 광고뷰의 높이 저장","            return cell","        }","        ","        \/\/ ... service 구현 ...","    }","}"],"syntax":"swift"}],"title":"Swift"},{"title":"Objective-C","content":[{"syntax":"objective-c","code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK.h>","","@implementation MyViewController","","\/\/ ... 구현 생략 ...","","#pragma mark - UITableViewDataSource","- (nonnull UITableViewCell *)tableView:(nonnull UITableView *)tableView cellForRowAtIndexPath:(nonnull NSIndexPath *)indexPath {","    BizBoardCell *cell = [tableView dequeueReusableCellWithIdentifier:adCellName forIndexPath:indexPath];","    if (cell) {","        self.adHeight = [cell adHeightWithWidth:self.view.bounds.size.width];","        return cell","    }","    ","    \/\/ ... service 구현 ...","}","@end"],"type":"codeListing"}]}],"type":"tabNavigator"},{"type":"heading","anchor":"4-UITableViewDelegate-구현","level":3,"text":"4. UITableViewDelegate 구현"},{"type":"paragraph","inlineContent":[{"text":"과정 3에서 저장했던 ","type":"text"},{"type":"strong","inlineContent":[{"text":"adHeight","type":"text"}]},{"text":" 값을 활용하여 cell의 높이를 설정하고, 과정 2에서 저장한 광고 응답인 ","type":"text"},{"type":"strong","inlineContent":[{"text":"nativeAd","type":"text"}]},{"text":" property를 willDisplay 시점에 데이터를 맵핑합니다.","type":"text"}]},{"tabs":[{"title":"Swift","content":[{"type":"codeListing","code":["import UIKit","import AdFitSDK","","final class MyViewController: UIViewController, \/*... 생략 ...*\/ {","    \/\/ ... property 생략 ...","    private var adHeight: CGFloat = 0","    private var nativeAd: AdFitNativeAd?","    ","    \/\/ ... 구현 생략 ...","    ","    \/\/ MARK: UITableViewDelegate","    ","    func tableView(_ tableView: UITableView, willDisplay cell: UITableViewCell, forRowAt indexPath: IndexPath) {","        if let cell = cell as? BizBoardCell {","            if let nativeAd = nativeAd {","                nativeAd.bind(cell)","            }","        }","    }","    ","    func tableView(_ tableView: UITableView, heightForRowAt indexPath: IndexPath) -> CGFloat {","        return adHeight","    }","    ","    func tableView(_ tableView: UITableView, estimatedHeightForRowAt indexPath: IndexPath) -> CGFloat {","        return adHeight","    }"],"syntax":"swift"}]},{"title":"Objective-C","content":[{"syntax":"objective-c","type":"codeListing","code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK.h>","","@implementation MyViewController","","\/\/ ... 구현 생략 ...","","#pragma mark - UITableViewDelegate","","- (void)tableView:(UITableView *)tableView willDisplayCell:(UITableViewCell *)cell forRowAtIndexPath:(NSIndexPath *)indexPath {","    BizBoardCell *bizCell = (BizBoardCell *)cell;","    [self.nativeAd bind:bizCell];","}","","- (CGFloat)tableView:(UITableView *)tableView heightForRowAtIndexPath:(NSIndexPath *)indexPath {","    return self.adHeight;","}","","- (CGFloat)tableView:(UITableView *)tableView estimatedHeightForRowAtIndexPath:(NSIndexPath *)indexPath {","    return self.adHeight;","}","","@end"]}]}],"type":"tabNavigator"}]}],"schemaVersion":{"major":0,"patch":0,"minor":3},"references":{"doc://AdFitSDK/documentation/AdFitSDK/BizBoardTemplate":{"abstract":[{"type":"text","text":"비즈보드 템플릿 광고 연동 과정을 소개합니다."}],"identifier":"doc:\/\/AdFitSDK\/documentation\/AdFitSDK\/BizBoardTemplate","url":"\/documentation\/adfitsdk\/bizboardtemplate","images":[{"identifier":"banner-card.png","type":"card"},{"type":"icon","identifier":"iphone.png"}],"type":"topic","role":"collectionGroup","title":"비즈보드 템플릿 광고 연동","kind":"article"},"ad-icon.png":{"variants":[{"traits":["2x","light"],"url":"\/images\/ad-icon@2x.png"}],"identifier":"ad-icon.png","alt":"A technology icon representing the SlothCreator framework.","type":"image"},"iphone.png":{"identifier":"iphone.png","type":"image","alt":"A technology icon representing the SlothCreator framework.","variants":[{"url":"\/images\/iphone.png","traits":["1x","light"]}]},"doc://AdFitSDK/documentation/AdFitSDK":{"kind":"symbol","type":"topic","url":"\/documentation\/adfitsdk","images":[{"type":"icon","identifier":"ad-icon.png"}],"identifier":"doc:\/\/AdFitSDK\/documentation\/AdFitSDK","title":"AdFitSDK","role":"collection","abstract":[{"type":"text","text":"AdFit SDK"}]},"banner-card.png":{"variants":[{"traits":["2x","light"],"url":"\/images\/banner-card@2x.png"}],"identifier":"banner-card.png","alt":"배너 광고 카드","type":"image"}}}