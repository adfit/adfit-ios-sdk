ar(_ animated: Bool) {","    super.viewWillAppear(animated)","    ","    nativeAdLoader = AdFitNativeAdLoader(clientId: \"광고단위를 입력해 주세요.\")","    nativeAdLoader.delegate = self","    nativeAdLoader?.loadAd()","}"]}]},{"title":"Objective-C","content":[{"type":"codeListing","syntax":"objective-c","code":["- (void)viewWillAppear:(BOOL)animated {","    [super viewWillAppear:animated];","    ","    self.nativeAdLoader = [[AdFitNativeAdLoader alloc] initWithClientId:@\"광고단위를 입력해 주세요.\" count:1 userObject:nil contentObject:nil];","    self.nativeAdLoader.delegate = self;","    [self.nativeAdLoader loadAdWithKeyword:nil];","}"]}]}]},{"anchor":"52-비즈보드-Margin-설정","level":3,"type":"heading","text":"5.2 비즈보드 Margin 설정"},{"type":"unorderedList","items":[{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"기본 값은 "},{"type":"strong","inlineContent":[{"type":"text","text":"UIEdgeInsets(top: 0, left: 16, bottom: 8, right: 16)"}]},{"type":"text","text":" 입니다."}]}]},{"content":[{"type":"paragraph","inlineContent":[{"type":"text","text":"높이 계산 로직에서 margin의 수치만큼, width나 height에서 제외됩니다. 그렇기 때문에 반드시, "},{"type":"strong","inlineContent":[{"type":"text","text":"adHeight(width: CGFloat)"}]},{"type":"text","text":" function을 호출하기 전에 margin을 설정해 주세요."}]}]}]},{"type":"tabNavigator","tabs":[{"title":"Swift","content":[{"type":"codeListing","syntax":"swift","code":["override func viewDidLoad() {","    super.viewDidLoad()","    \/\/ ...","    bizBoardTemplate.bgViewleftMargin = 56","    bizBoardTemplate.bgViewRightMargin = 16","    bizBoardTemplate.bgViewTopMargin = 10","    bizBoardTemplate.bgViewBottomMargin = 10","    \/\/ ...","}"]}]},{"title":"Objective-C","content":[{"type":"codeListing","syntax":"objective-c","code":["- (void)viewDidLoad {","    [super viewDidLoad];","    bizBoardTemplate.bgViewleftMargin = 56;","    bizBoardTemplate.bgViewRightMargin = 16;","    bizBoardTemplate.bgViewTopMargin = 10;","    bizBoardTemplate.bgViewBottomMargin = 10;","}"]}]}]},{"anchor":"