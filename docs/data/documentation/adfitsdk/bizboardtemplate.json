)","        \/\/ 비즈보드 비율에 맞는 높이 계산","        view.addSubview(bizBoardTemplate)","        NSLayoutConstraint.activate([","            bizBoardTemplate.topAnchor.constraint(equalTo: view.safeAreaLayoutGuide.topAnchor),","            bizBoardTemplate.leadingAnchor.constraint(equalTo: view.safeAreaLayoutGuide.leadingAnchor),","            bizBoardTemplate.trailingAnchor.constraint(equalTo: view.safeAreaLayoutGuide.trailingAnchor),","            bizBoardTemplate.heightAnchor.constraint(equalToConstant: adViewHeight)","        ])","    }","    ","    \/\/ MARK: AdFitNativeAdLoaderDelegate","    \/\/ ... 생략 ...","}"]}]},{"title":"Objective-C","content":[{"type":"codeListing","syntax":"objective-c","code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK-Swift.h>","","@implementation MyViewController","","- (void)viewDidLoad {","    [super viewDidLoad];","    ","    [self setupBizBoardTemplate];","    self.nativeAdLoader = \/* 생략 *\/","}","","\/\/ AutoLayout","-(void)setupBizBoardTemplate {","    self.bizBoardTemplate = [[BizBoardTemplate alloc] init];","    self.bizBoardTemplate.translatesAutoresizingMaskInfoConstraints = false;","    CGFloat adViewHeight = [self.bizBoardTemplate adHeightWithWidth:self.view.frame.size.width];","    \/\/ 비즈보드 비율에 맞는 높이 계산","    [self.view addSubview:self.bizBoardTemplate];","    [NSLayoutConstraint activateConstraints:@[","        [self.bizBoardTemplate.topAnchor constraintEqualToAnchor:self.view.safeAreaLayoutGuide.topAnchor],","        [self.bizBoardTemplate.leadingAnchor constraintEqualToAnchor:self.view.safeAreaLayoutGuide.leadingAnchor],","        [self.bizBoardTemplate.trailingAnchor constraintEqualToAnchor:self.view.safeAreaLayoutGuide.trailingAnchor],","        [self.bizBoardTemplate.heightAnchor constraintEqualToConstant:adViewHeight]","    ]];","}","","#pragma mark - AdFitNativeAdLoaderDelegate","\/\/ ... 생략 ...","@end"]}]}]},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"viewDidLoad"}]},{"type":"text","text":" 시점에 "},{"type":"strong","inlineContent":[{"type":"text","text":"BizBoardTemplate"}]},{"type":"text","text":"의 레이아웃을 설정합니다. "},{"type":"strong","inlineContent":[{"type":"text","text":"adHeight(width: CGFloat)"}]},{"type":"text","text":" function은 입력한 width를 기준으로 비즈보드 비율로 height를 계산합니다."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"위 코드의 예시는 autoLayout이나, 다른 방식으로 layout을 구성해도 상관없습니다. 하지만, width와 height의 비율이 비즈보드 이미지의 비율과 맞지 않는다면, 광고가 정상노출이 불가능할 수 있기 때문에 높이를 설정할 때 "},{"type":"strong","inlineContent":[{"type":"text","text":"adHeight(width: CGFloat)"}]},{"type":"text","text":" function을 활용해 주시길 바랍니다."}]},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"type":"text","text":"UITableViewCell"}]},{"type":"text","text":" 형태의 지원을 원하시는 분은 "},{"type":"reference","isActive":true,"identifier":"doc:\/\/AdFitSDK\/documentation\/AdFitSDK\/BizBoardTemplate+Cell"},{"type":"text","text":"를 참고 부탁드립니다."}]},{"anchor":"3-데이터-연결하기","level":2,"type":"heading","text":"3. 데이터 연결하기"},{"type":"paragraph","inlineContent":[{"type":"text","text":"뷰 구성이 끝나고, 광고 응답이 정상적으로 내려왔을 경우 데이터를 뷰에 넣어줘야 합니다."}]},{"type":"tabNavigator","tabs":[{"title":"Swift","content":[{"type":"codeListing","syntax":"swift","code":["import UIKit","import AdFitSDK","","final class MyViewController: UIViewController, AdFitNativeAdLoaderDelegate {","    private let bizBoardTemplate = BizBoardTemplate()","    \/\/ ... property 생략 ...","    ","    override func viewDidLoad() {","        super.viewDidLoad()","        ","        \/\/ ... 생략 ...","    }","    ","    \/\/ ... function 생략 ...","    ","    \/\/ MARK: AdFitNativeAdLoaderDelegate","    func nativeAdLoaderDidReceiveAd(_ nativeAd: AdFitNativeAd) {","        nativeAd.bind(bizBoardTemplate)","    }","    ","    func nativeAdLoaderFailToReceiveAd(_ nativeAdLoader: AdFitNativeAdLoader, error: Error) {","        print(\"광고 응답 에러: \\(error.localizedDescription)\")","        bizBoardTemplate.isHidden = true","    }","}"]}]},{"title":"Objective-C","content":[{"type":"codeListing","syntax":"objective-c","code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK-Swift.h>","","@implementation MyViewController","","- (void)viewDidLoad {","    [super viewDidLoad];","    ","    \/\/ ... 생략 ...","}","","\/\/ ... function 생략 ...","","#pragma mark - AdFitNativeAdLoaderDelegate","- (void)nativeAdLoaderDidReceiveAd:(AdFitNativeAd *)nativeAd {","    [nativeAd bind:self.bizBoardTemplate];","}","- (void)nativeAdLoaderDidFailToReceiveAd:(AdFitNativeAdLoader *)nativeAdLoader error:(NSError *)error {","    NSLog(@\"광고 응답 에러: %@\", [error localizedDescription]);","    [bizBoardTemplate setHidden:YES];","}","@end"]}]}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"응답으로 받은 nativeAd의 bind(_ nativeAdView: UIView)를 호출하면 데이터가 뷰에 맵핑됩니다."}]},{"anchor":"4-에러-코드","level":2,"type":"heading","text":"4. 에러 코드"},{"type":"paragraph","inlineContent":[{"type":"text","text":"광고 수신에 실패한 경우, nativeAdLoaderDidFailToReceiveAd delegate 메서드를 통해 에러 객체를 전달받을 수 있습니다."},{"type":"text","text":" "},{"type":"text","text":"각각의 에러에 대한 설명은 아래 표를 참고해 주세요."}]},{"rows":[[[{"type":"paragraph","inlineContent":[{"type":"text","text":"코드"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"메시지"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"설명"}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"1"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"clientId property is nil"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"배너광고만 해당됩니다. 비즈보드 광고는 해당 없습니다."}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"2"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"no ad to show"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"노출 가능한 광고가 없는 경우. 잠시 후 다시 광고 요청을 시도해 주세요."}]}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"3"}]}],[{"type":"paragraph","inlineContent":[{"ty