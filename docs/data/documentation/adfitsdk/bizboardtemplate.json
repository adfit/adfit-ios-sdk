egate = self;","    [self.nativeAdLoader loadAdWithKeyword:nil];","}","","#pragma mark - AdFitNativeAdLoaderDelegate","- (void)nativeAdLoaderDidReceiveAd:(AdFitNativeAd *)nativeAd {","    \/\/ 광고응답 -> nativeAd: AdFitNativeAd","}","","- (void)nativeAdLoaderDidFailToReceiveAd:(AdFitNativeAdLoader *)nativeAdLoader error:(NSError *)error {","    \/\/ 광고응답 실패 -> error 확인하기","}","@end"],"type":"codeListing"}],"title":"Objective-C"}]},{"inlineContent":[{"text":"광고 단위를 입력해서, loader를 생성하고, ","type":"text"},{"type":"strong","inlineContent":[{"type":"text","text":"loadAd"}]},{"text":" function을 호출합니다. 광고에 대한 응답을 받기 위해서는 ","type":"text"},{"type":"strong","inlineContent":[{"type":"text","text":"AdFitNativeAdLoaderDelegate"}]},{"type":"text","text":"를 상속받아서 구현한 function으로 응답을 받아 볼 수 있습니다."}],"type":"paragraph"},{"type":"heading","anchor":"2-%EB%A0%88%EC%9D%B4%EC%95%84%EC%9B%83-%EA%B5%AC%EC%84%B1%ED%95%98%EA%B8%B0","text":"2. 레이아웃 구성하기","level":2},{"inlineContent":[{"text":"Native 형식의 광고지만, 레이아웃에 대한 구성은 모두 SDK 내부에서 구성하여 정해진 템플릿 형식으로 뷰를 제공합니다.","type":"text"}],"type":"paragraph"},{"tabs":[{"content":[{"type":"codeListing","code":["import UIKit","import AdFitSDK","","final class MyViewController: UIViewController, AdFitNativeAdLoaderDelegate {","    private var nativeAdLoader: AdFitNativeAdLoader?","    private let bizboardTemplate = BizBoardTemplate() ","","    override func viewDidLoad() {","        super.viewDidLoad()","        ","        setUpBizBoardTemplate()","        nativeAdLoader = \/* 생략 *\/","    }","    ","    \/\/ AutoLayout","    func setUpBizBoardTemplate() {","        bizBoardTemplate.translatesAutoresizingMaskIntoConstraints = false","        let adViewHeight = bizBoardTemplate.adHeight(width: view.frame.width)","        \/\/ 비즈보드 비율에 맞는 높이 계산","        view.addSubview(bizBoardTemplate)","        NSLayoutConstraint.activate([","            bizBoardTemplate.topAnchor.constraint(equalTo: view.safeAreaLayoutGuide.topAnchor),","            bizBoardTemplate.leadingAnchor.constraint(equalTo: view.safeAreaLayoutGuide.leadingAnchor),","            bizBoardTemplate.trailingAnchor.constraint(equalTo: view.safeAreaLayoutGuide.trailingAnchor),","            bizBoardTemplate.heightAnchor.constraint(equalToConstant: adViewHeight)","        ])","    }","    ","    \/\/ MARK: AdFitNativeAdLoaderDelegate","    \/\/ ... 생략 ...","}"],"syntax":"swift"}],"title":"Swift"},{"content":[{"type":"codeListing","code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK-Swift.h>","","@implementation MyViewController","","- (void)viewDidLoad {","    [super viewDidLoad];","    ","    [self setupBizBoardTemplate];","    self.nativeAdLoader = \/* 생략 *\/","}","","\/\/ AutoLayout","-(void)setupBizBoardTemplate {","    self.bizBoardTemplate = [[BizBoardTemplate alloc] init];","    self.bizBoardTemplate.translatesAutoresizingMaskInfoConstraints = false;","    CGFloat adViewHeight = [self.bizBoardTemplate adHeightWithWidth:self.view.frame.size.width];","    \/\/ 비즈보드 비율에 맞는 높이 계산","    [self.view addSubview:self.bizBoardTemplate];","    [NSLayoutConstraint activateConstraints:@[","        [self.bizBoardTemplate.topAnchor constraintEqualToAnchor:self.view.safeAreaLayoutGuide.topAnchor],","        [self.bizBoardTemplate.leadingAnchor constraintEqualToAnchor:self.view.safeAreaLayoutGuide.leadingAnchor],","        [self.bizBoardTemplate.trailingAnchor constraintEqualToAnchor:self.view.safeAreaLayoutGuide.trailingAnchor],","        [self.bizBoardTemplate.heightAnchor constraintEqualToConstant:adViewHeight]","    ]];","}","","#pragma mark - AdFitNativeAdLoaderDelegate","\/\/ ... 생략 ...","@end"],"syntax":"objective-c"}],"title":"Objective-C"}],"type":"tabNavigator"},{"type":"paragraph","inlineContent":[{"type":"strong","inlineContent":[{"text":"viewDidLoad","type":"text"}]},{"text":" 시점에 ","type":"text"},{"type":"strong","inlineContent":[{"type":"text","text":"BizBoardTemplate"}]},{"type":"text","text":"의 레이아웃을 설정합니다. "},{"inlineContent":[{"text":"adHeight(width: CGFloat)","type":"text"}],"type":"strong"},{"type":"text","text":" function은 입력한 width를 기준으로 비즈보드 비율로 height를 계산합니다."}]},{"inlineContent":[{"text":"위 코드의 예시는 autoLayout이나, 다른 방식으로 layout을 구성해도 상관없습니다. 하지만, width와 height의 비율이 비즈보드 이미지의 비율과 맞지 않는다면, 광고가 정상노출이 불가능할 수 있기 때문에 높이를 설정할 때 ","type":"text"},{"inlineContent":[{"type":"text","text":"adHeight(width: CGFloat)"}],"type":"strong"},{"text":" function을 활용해 주시길 바랍니다.","type":"text"}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"inlineContent":[{"type":"text","text":"UITableViewCell"}],"type":"strong"},{"text":" 형태의 지원을 원하시는 분은 ","type":"text"},{"isActive":true,"type":"reference","identifier":"doc:\/\/AdFitSDK\/documentation\/AdFitSDK\/BizBoardTemplate+Cell"},{"text":"를 참고 부탁드립니다.","type":"text"}]},{"level":2,"type":"heading","text":"3. 데이터 연결하기","anchor":"3-%EB%8D%B0%EC%9D%B4%ED%84%B0-%EC%97%B0%EA%B2%B0%ED%95%98%EA%B8%B0"},{"inlineContent":[{"text":"뷰 구성이 끝나고, 광고 응답이 정상적으로 내려왔을 경우 데이터를 뷰에 넣어줘야 합니다.","type":"text"}],"type":"paragraph"},{"tabs":[{"title":"Swift","content":[{"syntax":"swift","type":"codeListing","code":["import UIKit","import AdFitSDK","","final class MyViewController: UIViewController, AdFitNativeAdLoaderDelegate {","    private let bizBoardTemplate = BizBoardTemplate()","    \/\/ ... property 생략 ...","    ","    override func viewDidLoad() {","        super.viewDidLoad()","        ","        \/\/ ... 생략 ...","    }","    ","    \/\/ ... function 생략 ...","    ","    \/\/ MARK: AdFitNativeAdLoaderDelegate","    func nativeAdLoaderDidReceiveAd(_ nativeAd: AdFitNativeAd) {","        nativeAd.bind(bizBoardTemplate)","    }","    ","    func nativeAdLoaderFailToReceiveAd(_ nativeAdLoader: AdFitNativeAdLoader, error: Error) {","        print(\"광고 응답 에러: \\(error.localizedDescription)\")","        bizBoardTemplate.isHidden = true","    }","}"]}]},{"title":"Objective-C","content":[{"syntax":"objective-c","code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK-Swift.h>","","@implementation MyViewController","","- (void)viewDidLoad {","    [super viewDidLoad];","    ","    \/\/ ... 생략 ...","}","","\/\/ ... function 생략 ...","","#pragma mark - AdFitNativeAdLoaderDelegate","- (void)nativeAdLoaderDidReceiveAd:(AdFitNativeAd *)nativeAd {","    [nativeAd bind:self.bizBoardTemplate];","}","- (void)nativeAdLoaderDidFailToReceiveAd:(AdFitNativeAdLoader *)nativeAdLoader error:(NSError *)error {","    NSLog(@\"광고 응답 에러: %@\", [error localizedDescription]);","    [bizBoardTemplate setHidden:YES];","}","@end"],"type":"codeListing"}]}],"type":"tabNavigator"},{"type":"paragraph","inlineContent":[{"text":"응답으로 받은 nativeAd의 bind(_ nativeAdView: UIView)를 호출하면 데이터가 뷰에 맵핑됩니다.","type":"text"}]},{"type":"heading","anchor":"4-%EC%97%90%EB%9F%AC-%EC%BD%94%EB%93%9C","level":2,"text":"4. 에러 코드"},{"inlineContent":[{"type":"text","text":"광고 수신에 실패한 경우, nativeAdLoaderDidFailToReceiveAd delegate 메서드를 통해 에러 객체를 전달받을 수 있습니다."},{"text":" ","type":"text"},{"text":"각각의 에러에 대한 설명은 아래 표를 참고해 주세요.","type":"text"}],"type":"paragraph"},{"header":"row","alignments":["center","unset","unset"],"rows":[[[{"type":"paragraph","inlineContent":[{"text":"코드","type":"text"}]}],[{"type":"paragraph","inlineContent":[{"text":"메시지","type":"text"}]}],[{"inlineContent":[{"type":"text","text":"설명"}],"type":"paragraph"}]],[[{"type":"paragraph","inlineContent":[{"type":"text","text":"1"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"clientId property is nil"}]}],[{"inlineContent":[{"text":"배너광고만 해당됩니다. 비즈보드 광고는 해당 없습니다.","type":"text"}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"2"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"no ad to show"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"노출 가능한 광고가 없는 경우. 잠시 후 다시 광고 요청을 시도해 주세요."}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"3"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"invalid ad received"}]}],[{"type":"paragrap