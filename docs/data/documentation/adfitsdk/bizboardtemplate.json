","        nativeAdLoader?.delegate = self","        nativeAdLoader?.loadAd()","    }","    ","    \/\/ MARK: AdFitNativeAdLoaderDelegate","    func nativeAdLoaderDidReceiveAd(_ nativeAd: AdFitNativeAd) {","        \/\/ 광고응답 -> nativeAd: AdFitNativeAd","    }","    ","    func nativeAdLoaderDidFailToReceiveAd(_ nativeAdLoader: AdFitNativeAdLoader, error: Error) {","        \/\/ 광고응답 실패 -> error 확인하기","    }","}"]}]},{"content":[{"code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK-Swift.h>","","@implementation MyViewController","","- (void)viewDidLoad {","    [super viewDidLoad];","    self.nativeAdLoader = [[AdFitNativeAdLoader alloc] initWithClientId:@\"광고단위를 입력해 주세요.\" count:1 userObject:nil contentObject:nil];","    self.nativeAdLoader.delegate = self;","    [self.nativeAdLoader loadAdWithKeyword:nil];","}","","#pragma mark - AdFitNativeAdLoaderDelegate","- (void)nativeAdLoaderDidReceiveAd:(AdFitNativeAd *)nativeAd {","    \/\/ 광고응답 -> nativeAd: AdFitNativeAd","}","","- (void)nativeAdLoaderDidFailToReceiveAd:(AdFitNativeAdLoader *)nativeAdLoader error:(NSError *)error {","    \/\/ 광고응답 실패 -> error 확인하기","}","@end"],"syntax":"objective-c","type":"codeListing"}],"title":"Objective-C"}],"type":"tabNavigator"},{"inlineContent":[{"text":"광고 단위를 입력해서, loader를 생성하고, ","type":"text"},{"inlineContent":[{"text":"loadAd","type":"text"}],"type":"strong"},{"type":"text","text":" function을 호출합니다. 광고에 대한 응답을 받기 위해서는 "},{"inlineContent":[{"text":"AdFitNativeAdLoaderDelegate","type":"text"}],"type":"strong"},{"type":"text","text":"를 상속받아서 구현한 function으로 응답을 받아 볼 수 있습니다."}],"type":"paragraph"},{"text":"2. 레이아웃 구성하기","anchor":"2-레이아웃-구성하기","level":2,"type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"Native 형식의 광고지만, 레이아웃에 대한 구성은 모두 SDK 내부에서 구성하여 정해진 템플릿 형식으로 뷰를 제공합니다."}]},{"tabs":[{"content":[{"syntax":"swift","code":["import UIKit","import AdFitSDK","","final class MyViewController: UIViewController, AdFitNativeAdLoaderDelegate {","    private var nativeAdLoader: AdFitNativeAdLoader?","    private let bizboardTemplate = BizBoardTemplate() ","","    override func viewDidLoad() {","        super.viewDidLoad()","        ","        setUpBizBoardTemplate()","        nativeAdLoader = \/* 생략 *\/","    }","    ","    \/\/ AutoLayout","    func setUpBizBoardTemplate() {","        bizBoardTemplate.translatesAutoresizingMaskIntoConstraints = false","        let adViewHeight = bizBoardTemplate.adHeight(width: view.frame.width)","        \/\/ 비즈보드 비율에 맞는 높이 계산","        view.addSubview(bizBoardTemplate)","        NSLayoutConstraint.activate([","            bizBoardTemplate.topAnchor.constraint(equalTo: view.safeAreaLayoutGuide.topAnchor),","            bizBoardTemplate.leadingAnchor.constraint(equalTo: view.safeAreaLayoutGuide.leadingAnchor),","            bizBoardTemplate.trailingAnchor.constraint(equalTo: view.safeAreaLayoutGuide.trailingAnchor),","            bizBoardTemplate.heightAnchor.constraint(equalToConstant: adViewHeight)","        ])","    }","    ","    \/\/ MARK: AdFitNativeAdLoaderDelegate","    \/\/ ... 생략 ...","}"],"type":"codeListing"}],"title":"Swift"},{"content":[{"type":"codeListing","code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK-Swift.h>","","@implementation MyViewController","","- (void)viewDidLoad {","    [super viewDidLoad];","    ","    [self setupBizBoardTemplate];","    self.nativeAdLoader = \/* 생략 *\/","}","","\/\/ AutoLayout","-(void)setupBizBoardTemplate {","    self.bizBoardTemplate = [[BizBoardTemplate alloc] init];","    self.bizBoardTemplate.translatesAutoresizingMaskInfoConstraints = false;","    CGFloat adViewHeight = [self.bizBoardTemplate adHeightWithWidth:self.view.frame.size.width];","    \/\/ 비즈보드 비율에 맞는 높이 계산","    [self.view addSubview:self.bizBoardTemplate];","    [NSLayoutConstraint activateConstraints:@[","        [self.bizBoardTemplate.topAnchor constraintEqualToAnchor:self.view.safeAreaLayoutGuide.topAnchor],","        [self.bizBoardTemplate.leadingAnchor constraintEqualToAnchor:self.view.safeAreaLayoutGuide.leadingAnchor],","        [self.bizBoardTemplate.trailingAnchor constraintEqualToAnchor:self.view.safeAreaLayoutGuide.trailingAnchor],","        [self.bizBoardTemplate.heightAnchor constraintEqualToConstant:adViewHeight]","    ]];","}","","#pragma mark - AdFitNativeAdLoaderDelegate","\/\/ ... 생략 ...","@end"],"syntax":"objective-c"}],"title":"Objective-C"}],"type":"tabNavigator"},{"inlineContent":[{"inlineContent":[{"text":"viewDidLoad","type":"text"}],"type":"strong"},{"type":"text","text":" 시점에 "},{"type":"strong","inlineContent":[{"text":"BizBoardTemplate","type":"text"}]},{"type":"text","text":"의 레이아웃을 설정합니다. "},{"inlineContent":[{"type":"text","text":"adHeight(width: CGFloat)"}],"type":"strong"},{"type":"text","text":" function은 입력한 width를 기준으로 비즈보드 비율로 height를 계산합니다."}],"type":"paragraph"},{"type":"paragraph","inlineContent":[{"type":"text","text":"위 코드의 예시는 autoLayout이나, 다른 방식으로 layout을 구성해도 상관없습니다. 하지만, width와 height의 비율이 비즈보드 이미지의 비율과 맞지 않는다면, 광고가 정상노출이 불가능할 수 있기 때문에 높이를 설정할 때 "},{"type":"strong","inlineContent":[{"type":"text","text":"adHeight(width: CGFloat)"}]},{"text":" function을 활용해 주시길 바랍니다.","type":"text"}]},{"type":"paragraph","inlineContent":[{"inlineContent":[{"text":"UITableViewCell","type":"text"}],"type":"strong"},{"text":" 형태의 지원을 원하시는 분은 ","type":"text"},{"identifier":"doc:\/\/AdFitSDK\/documentation\/AdFitSDK\/BizBoardTemplate+Cell","isActive":true,"type":"reference"},{"text":"를 참고 부탁드립니다.","type":"text"}]},{"type":"heading","text":"3. 데이터 연결하기","anchor":"3-데이터-연결하기","level":2},{"inlineContent":[{"type":"text","text":"뷰 구성이 끝나고, 광고 응답이 정상적으로 내려왔을 경우 데이터를 뷰에 넣어줘야 합니다."}],"type":"paragraph"},{"type":"tabNavigator","tabs":[{"content":[{"type":"codeListing","syntax":"swift","code":["import UIKit","import AdFitSDK","","final class MyViewController: UIViewController, AdFitNativeAdLoaderDelegate {","    private let bizBoardTemplate = BizBoardTemplate()","    \/\/ ... property 생략 ...","    ","    override func viewDidLoad() {","        super.viewDidLoad()","        ","        \/\/ ... 생략 ...","    }","    ","    \/\/ ... function 생략 ...","    ","    \/\/ MARK: AdFitNativeAdLoaderDelegate","    func nativeAdLoaderDidReceiveAd(_ nativeAd: AdFitNativeAd) {","        nativeAd.bind(bizBoardTemplate)","    }","    ","    func nativeAdLoaderFailToReceiveAd(_ nativeAdLoader: AdFitNativeAdLoader, error: Error) {","        print(\"광고 응답 에러: \\(error.localizedDescription)\")","        bizBoardTemplate.isHidden = true","    }","}"]}],"title":"Swift"},{"content":[{"syntax":"objective-c","type":"codeListing","code":["#import \"MyViewController.h\"","#import <AdFitSDK\/AdFitSDK-Swift.h>","","@implementation MyViewController","","- (void)viewDidLoad {","    [super viewDidLoad];","    ","    \/\/ ... 생략 ...","}","","\/\/ ... function 생략 ...","","#pragma mark - AdFitNativeAdLoaderDelegate","- (void)nativeAdLoaderDidReceiveAd:(AdFitNativeAd *)nativeAd {","    [nativeAd bind:self.bizBoardTemplate];","}","- (void)nativeAdLoaderDidFailToReceiveAd:(AdFitNativeAdLoader *)nativeAdLoader error:(NSError *)error {","    NSLog(@\"광고 응답 에러: %@\", [error localizedDescription]);","    [bizBoardTemplate setHidden:YES];","}","@end"]}],"title":"Objective-C"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"응답으로 받은 nativeAd의 bind(_ nativeAdView: UIView)를 호출하면 데이터가 뷰에 맵핑됩니다."}]},{"level":2,"anchor":"4-에러-코드","text":"4. 에러 코드","type":"heading"},{"type":"paragraph","inlineContent":[{"type":"text","text":"광고 수신에 실패한 경우, nativeAdLoaderDidFailToReceiveAd delegate 메서드를 통해 에러 객체를 전달받을 수 있습니다."},{"text":" ","type":"text"},{"text":"각각의 에러에 대한 설명은 아래 표를 참고해 주세요.","type":"text"}]},{"type":"table","header":"row","rows":[[[{"inlineContent":[{"type":"text","text":"코드"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"메시지"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"설명"}],"type":"paragraph"}]],[[{"inlineContent":[{"text":"1","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"clientId property is nil","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"배너광고만 해당됩니다. 비즈보드 광고는 해당 없습니다."}],"type":"paragraph"}]],[[{"inlineContent":[{"type":"text","text":"2"}],"type":"paragraph"}],[{"inlineContent":[{"type":"text","text":"no ad to show"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"text":"노출 가능한 광고가 없는 경우. 잠시 후 다시 광고 요청을 시도해 주세요.","type":"text"}]}]],[[{"type":"paragraph","inlineContent":[{"text":"3","type":"text"}]}],[{"inlineContent":[{"text":"invalid ad received","type":"text"}],"type":"paragraph"}],[{"inlineContent":[{"text":"유효하지 않은 광고를 받은 경우. AdFit에 문의해 주세요.","type":"text"}],"type":"paragraph"}]],[[{"type":"paragraph","inlineContent":[{"text":"5","type":"text"}]}],[{"inlineContent":[{"type":"text","text":"attempted to load ad too frequently"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"text":"과도하게 짧은 시간 간격으로 광고를 재요청한 경우.","type":"text"}]}]],[[{"inlineContent":[{"type":"text","text":"6"}],"type":"paragraph"}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"HTTP failed"}]}],[{"type":"paragraph","inlineContent":[{"type":"text","text":"HTTP 에러. 계속해서 반복 발생하는 경우 AdFit에 문의해 주세요."}]}]]],"alignments":["center","unset","unset"]},{"type":"heading","anchor":"5-Advance-Setting","level":2,"text":"5. Advance Setting"},{"type":"paragraph","inlineContent":[{"type":"text","text":"bizBoardTemplate의 레이아웃 변경이나 광고 갱신 시점 등 추가적인 고려사항에 대한 내용을 담고 있습니다."}]},{"anchor":"51-광고의-갱신-시점","level":3,"text":"5.1 광고의 갱신 시점","type":"heading"},{"items":[{"content":[{"inlineContent":[{"type":"strong","inlineContent":[{"text":"AdFitNativeAdLoader","type":"text"}]},{"text":"와 ","type":"text"},{"type":"strong","inlineContent":[{"type":"text","text":"AdFitNativeAd"}]},{"text":" 인스턴스는 한번 광고를 요청한 후 뷰의 라이프사이클 동안 유지됩니다.","type":"text"}],"type":"paragraph"}]},{"content":[{"type":"paragraph","inlineContent":[{"text":"서비스의 UI와 UX에 따라 비즈보드 갱신 시점이 달라질 수 있는데, 보통은 뷰의 전환이 일어